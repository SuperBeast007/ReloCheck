<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ReloCheck.Views.Search"
             xmlns:local="clr-namespace:ReloCheck"
             xmlns:toolkit="https://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             Title="Search" Shell.NavBarIsVisible="False" BackgroundColor="{StaticResource Gray50}" Background="{AppThemeBinding Light={StaticResource Gray50}, Dark=#1D1A22}">

    <ScrollView>
        <VerticalStackLayout>
            <SearchBar x:Name="SearchBarN"
               Placeholder="Search" 
               BackgroundColor="White"/>


        </VerticalStackLayout>
    </ScrollView>


</ContentPage>




